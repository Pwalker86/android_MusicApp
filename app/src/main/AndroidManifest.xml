<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AlbumListActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ArtistListActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SongListActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NowPlayingActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AlbumDetailsActivity"
            android:parentActivityName=".AlbumListActivity" />
        <activity
            android:name=".ArtistDetailsActivity"
            android:parentActivityName=".ArtistListActivity" />
    </application>

</manifest>